    "@types/d3-geo": "^3.0.3",
    "canvas": "2.10.2",
    "eslint": "^8.29.0",
    "pixelmatch": "^5.3.0",
    "pngjs": "^6.0.0",
    "rimraf": "^4.4.0",
    "rollup": "^3.19.1",
    "tape": "^5.6.1",
    "topojson-client": "^3.1.0",
    "typescript": "^4.9.5"
